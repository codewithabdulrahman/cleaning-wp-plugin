# Booking Widget
msgid "Book Your Cleaning Service"
msgstr "Κλείστε Υπηρεσία Καθαρισμού"

# Steps
msgid "Location"
msgstr "Τοποθεσία"
msgid "Service"
msgstr "Υπηρεσία"
msgid "Details"
msgstr "Λεπτομέρειες"
msgid "Date & Time"
msgstr "Ημερομηνία & Ώρα"
